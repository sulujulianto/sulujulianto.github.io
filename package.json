    "arg": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "didyoumean": "^1.2.2",
    "dlv": "^1.1.3",
    "fast-glob": "^3.3.3",
    "is-glob": "^4.0.3",
    "jiti": "^2.6.1",
    "normalize-path": "^3.0.0",
    "postcss": "^8.5.6",
    "postcss-js": "^5.0.1",
    "postcss-load-config": "^5.1.0",
