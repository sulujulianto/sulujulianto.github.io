{
  "name": "sulujulianto-portfolio",
  "version": "1.0.0",
  "description": "Personal portfolio website with React, TypeScript, and Tailwind CSS for multi-language support.",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tailwind:build": "tailwindcss -i ./assets/css/main.css -o ./assets/css/output.css",
    "tailwind:watch": "tailwindcss -i ./assets/css/main.css -o ./assets/css/output.css --watch",
    "ts:build": "tsc -p tsconfig.runtime.json",
    "blog:dev": "cd blog-fuwari && pnpm dev",
    "blog:build": "cd blog-fuwari && pnpm build",
    "blog:doctor": "cd blog-fuwari && pnpm doctor"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "tailwind-css",
    "multi-language",
    "personal-website"
  ],
  "author": "Sulu Edward Julianto",
